<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/clr_bg"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:background="@color/white"
        android:orientation="vertical">

        <TextView
            android:id="@+id/release_jo_auto_sel"
            style="@style/new_job_order_text"
            android:text="自动匹配符合条件劳方" />

        <View style="@style/divid" />

        <TextView
            android:id="@+id/manul_select_labor_tv"
            style="@style/new_job_order_text"
            android:text="手动选择劳方" />

        <View style="@style/divid" />

        <TextView
            android:id="@+id/collect_tv"
            style="@style/new_job_order_text"
            android:text="去我的收藏选择劳方" />
    </LinearLayout>


</LinearLayout>